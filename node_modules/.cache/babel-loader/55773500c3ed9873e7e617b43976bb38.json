ext-center\">\n    <v-container>\n      <v-row justify=\"center\">\n        <v-col cols=\"8\">\n          <v-container class=\"max-width\">\n            <v-btn :disabled=\"disabled\" @click=\"previous\" icon color=\"error\" width=\"40\"><v-icon size=\"40\" class=\"white\">mdi-arrow-left-circle</v-icon></v-btn>\n            <v-btn @click=\"next\" icon color=\"error\" width=\"40\"><v-icon size=\"40\" class=\"white\">mdi-arrow-right-circle</v-icon></v-btn>\n          </v-container>\n        </v-col>\n      </v-row>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport { getPokemons } from '../api/api'\nimport store from '../store/index'\n  export default {\n    data () {\n      return {\n        nextUrl: null,\n        prevUrl: null,\n        pokemons: [],\n        disabled: false,\n      }\n    },\n\n    computed: {\n      getNextUrl() {\n        return store.getters.nextUrl\n      },\n      getPrevUrl() {\n        return store.getters.prevUrl\n      },\n    },\n\n    created() {\n      \n      getPokemons('https://pokeapi.co/api/v2/pokemon/?offset=0&limit=12')\n      .then((res) => {\n        store.commit(\"SET_NEXT_URL\", res.next)\n        store.commit(\"SET_PREV_URL\", res.previous)\n        if (store.getters.prevUrl === null) {\n            this.disabled = true\n          } else {\n            this.disabled = false\n          }\n        console.log('Prev in created', res.previous)\n        res.results.forEach(pokemon => {\n              pokemon.id = pokemon.url.split('/').filter(function(part) { return !!part }).pop();\n              this.pokemons.push(pokemon);\n            });\n            console.log(this.pokemons)\n            store.commit(\"SET_POKEMONS\", this.pokemons)\n      })\n    },\n\n    methods: {\n    next() {\n        getPokemons(store.getters.nextUrl)\n        .then((res) => {\n          this.pokemons = []\n          console.log('Next url', store.getters.nextUrl)\n          store.commit(\"SET_NEXT_URL\", res.next)\n          store.commit(\"SET_PREV_URL\", res.previous)\n          if (store.getters.prevUrl === null) {\n            this.disabled = true\n          } else {\n            this.disabled = false\n          }\n\n          res.results.forEach(pokemon => {\n              pokemon.id = pokemon.url.split('/').filter(function(part) { return !!part }).pop();\n              this.pokemons.push(pokemon);\n            });\n            store.commit(\"SET_POKEMONS\", this.pokemons)\n            console.log(this.pokemons)\n            \n        })\n    },\n\n    previous() {\n\n        getPokemons(store.getters.prevUrl)\n        .then((res) => {\n          this.pokemons = []\n          console.log('Prev url', store.getters.prevUrl)\n          store.commit(\"SET_NEXT_URL\", res.next)\n          store.commit(\"SET_PREV_URL\", res.previous)\n          if (store.getters.prevUrl === null) {\n            this.disabled = true\n          } else {\n            this.disabled = false\n          }\n\n          res.results.forEach(pokemon => {\n              pokemon.id = pokemon.url.split('/').filter(function(part) { return !!part }).pop();\n              this.pokemons.push(pokemon);\n            });\n            store.commit(\"SET_POKEMONS\", this.pokemons)\n            console.log(this.pokemons)\n            \n        })\n    },\n  },\n  }\n</script>\n\n"],"sourceRoot":"src/components"}]}