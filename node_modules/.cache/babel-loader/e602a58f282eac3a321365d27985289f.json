,QAAA;AACA,OAFA;AAGA,KAfA;AAgBA,IAAA,OAhBA,mBAgBA,QAhBA,EAgBA;AACA,WAAA,IAAA,GAAA,QAAA,CAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,iBAAA,EAAA,KAAA,IAAA,EAFA,CAGA;AACA;AACA;AACA;AACA;AACA;AAxBA,GA9BA;AAwDA,EAAA,OAxDA,qBAwDA;AACA,SAAA,OAAA;AACA;AA1DA,CAAA","sourcesContent":["<template>\n  \n<div class=\"text-xs-center pt-2 md4 lg4\">\n      <!-- I removed v-model and added @input and :value -->\n      <v-pagination\n        @input=\"paginationChangeHandler\"\n        :value=\"pagination.page\"\n        :length=\"pages\"\n        :total-visible=\"7\"\n        circle\n      ></v-pagination>\n    </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      selectedRow: undefined,\n\n      search: \"\",\n      loading: false,\n      // i set default totalItems and page here, this fixed a lot of strange behaviour\n      pagination: { totalItems: 964, cardsPerPage: 9, page: 1 },\n      data: [],\n      expand: false,\n      selected: [],\n    };\n  },\n\n  computed: {\n    pages() {\n      if (\n        this.pagination.cardsPerPage == null ||\n        this.pagination.totalItems == null\n      ) {\n        return 0;\n      }\n\n      return Math.ceil(\n        this.pagination.totalItems / this.pagination.cardsPerPage\n      );\n    }\n  },\n\n  methods: {\n    // i added this (gets called on pagination components)\n    paginationChangeHandler(pageNumber) {\n      this.pagination.page = pageNumber;\n      this.getData();\n    },\n\n    getData() {\n      let url = `https://pokeapi.co/api/v2/pokemon?offset=${this.pagination.page - 1}&limit=${this.pagination.cardsPerPage}`;\n\n      // if you use es6 arrow function you dont need to do that = this.\n      // this will work inside the function\n      axios.get(url).then(response => {\n        this.setData(response);\n      });\n    },\n    setData(response) {\n      this.data = response.data;\n      console.log('Data in setData', this.data)\n      // unless this.pagination.totalItems changes\n      // on requests, you dont need to set it here.\n      // I set it up in vues data method.\n      // You can remove all these comments :)\n      // this.pagination.totalItems = 200;\n    }\n  },\n  mounted() {\n    this.getData();\n  }\n};\n</script>\n"],"sourceRoot":"src/components"}]}