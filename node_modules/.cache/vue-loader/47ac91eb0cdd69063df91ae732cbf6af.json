{"remainingRequest":"/Users/kasia/pokedex/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/kasia/pokedex/src/components/Filter.vue?vue&type=style&index=0&id=e12614b0&scoped=true&lang=css&","dependencies":[{"path":"/Users/kasia/pokedex/src/components/Filter.vue","mtime":1587018275823},{"path":"/Users/kasia/pokedex/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/kasia/pokedex/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/kasia/pokedex/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/kasia/pokedex/node_modules/vuetify-loader/lib/loader.js","mtime":1574476662000},{"path":"/Users/kasia/pokedex/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/kasia/pokedex/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Filter.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiMA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA","file":"Filter.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <v-container fluid>\n    <div id=\"selectType\">\n      <v-btn @click=\"pokedexMode\" class=\"elevation-10\">Pokedex List</v-btn>\n      <v-select :items=\"items\" label=\"Solo field\" dense solo v-model=\"type\" @change=\"getType()\"></v-select>\n      <v-btn @click=\"filterMode\" color=\"error\" class=\"elevation-10\">Types Filter</v-btn>\n    </div>\n\n    <div id=\"cards\">\n      <div v-for=\"element in this.typePokemons\" :key=\"element.pokemon.name\">\n        <v-hover v-slot:default=\"{ hover }\">\n          <v-card class=\"mx-auto d-flex flex-wrap elevation-6 card\" text raised @click.stop=\"loadDialog(element.id)\">\n            <v-col cols=\"auto\">\n              <v-img class=\"align-end image\" :src=\"`https://pokeres.bastionbot.org/images/pokemon/${element.id}.png`\"> </v-img>\n            </v-col>\n            <v-expand-transition>\n              <div v-if=\"hover\" class=\"d-flex transition-fast-in-fast-out red v-card--reveal white--text\" id=\"names\">\n                {{ element.pokemon.name }}\n              </div>\n            </v-expand-transition>\n          </v-card>\n        </v-hover>\n      </div>\n    </div>\n\n    <!-- Dialog - pokemon info -->\n    <v-dialog v-model=\"dialog\" max-width=\"420\" max-height=\"470\">\n      <v-card width=\"420\" height=\"470\" class=\"elevation-12\">\n        <v-col cols=\"auto\" id=\"dialogInfo\">\n          <v-spacer></v-spacer>\n          <v-avatar size=\"120\" class=\"grey lighten-3\">\n            <v-img class=\"align-end\" :src=\"`https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/${this.pokemon_ID}.png`\" width=\"150\" height=\"150\"></v-img>\n          </v-avatar>\n\n          <v-card-title id=\"titleDialog\">{{ this.pokemon_name }} </v-card-title>\n          <v-card-subtitle\n            ><v-chip class=\"ma-2\" dark color=\"#3B4CCA\" v-for=\"type in this.pokemon_types\" :key=\"type\"> {{ type }} </v-chip></v-card-subtitle\n          >\n          <v-row></v-row>\n\n          <v-card-text class=\"pokemonDetail\">\n            <v-row>Base experience: {{ this.pokemon_baseExp }} XP</v-row>\n            <v-row>Height: {{ this.pokemon_height / 10 }} m</v-row>\n            <v-row>Weight: {{ this.pokemon_weight / 10 }} kg</v-row>\n            <div id=\"abilities\">\n            Abilities\n            <v-row\n              ><v-chip class=\"ma-2\" color=\"amber accent-4\" v-for=\"ability in this.pokemon_abilities\" :key=\"ability\"> {{ ability }} </v-chip></v-row\n            >\n            </div>\n          </v-card-text>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn color=\"green darken-1\" text @click=\"dialog = false\">\n              Choose you!\n            </v-btn>\n          </v-card-actions>\n        </v-col>\n      </v-card>\n    </v-dialog>\n  </v-container>\n</template>\n<script>\nimport { getPokemons } from \"../api/api\";\nexport default {\n  data() {\n    return {\n      items: [\"Electric\", \"Grass\", \"Fire\", \"Water\"],\n      type: \"Electric\",\n      electric: [],\n      grass: [],\n      fire: [],\n      water: [],\n      typePokemons: [],\n\n      dialog: false,\n\n      pokemon_ID: null,\n      pokemon_name: \"\",\n      pokemon_types: [],\n      pokemon_baseExp: null,\n      pokemon_weight: null,\n      pokemon_height: null,\n      pokemon_abilities: [],\n\n      dialogUrl: null,\n    };\n  },\n\n  created() {\n    this.getType();\n  },\n\n  methods: {\n    filterMode(e) {\n      e.preventDefault();\n\n      this.$router.push(\"/filter\");\n    },\n    pokedexMode(e) {\n      e.preventDefault();\n\n      this.$router.push(\"/\");\n    },\n    getType() {\n      this.typePokemons = [];\n      if (this.type === \"Electric\") {\n        this.getElectricPokemons();\n        this.typePokemons = this.electric;\n      } else if (this.type === \"Grass\") {\n        this.getGrassPokemons();\n        this.typePokemons = this.grass;\n      } else if (this.type === \"Fire\") {\n        this.getFirePokemons();\n        this.typePokemons = this.fire;\n      } else if (this.type === \"Water\") {\n        this.getWaterPokemons();\n        this.typePokemons = this.water;\n      }\n    },\n\n    getPoke(pUrl, arr) {\n      this.typePokemons = [];\n      getPokemons(pUrl).then((res) => {\n        res.pokemon.forEach((element) => {\n          element.id = element.pokemon.url\n            .split(\"/\")\n            .filter(function (part) {\n              return !!part;\n            })\n            .pop();\n          arr.push(element);\n        });\n      });\n    },\n\n    getElectricPokemons() {\n      this.getPoke(\"https://pokeapi.co/api/v2/type/electric\", this.electric);\n    },\n\n    getGrassPokemons() {\n      this.getPoke(\"https://pokeapi.co/api/v2/type/grass\", this.grass);\n    },\n\n    getFirePokemons() {\n      this.getPoke(\"https://pokeapi.co/api/v2/type/fire\", this.fire);\n    },\n\n    getWaterPokemons() {\n      this.getPoke(\"https://pokeapi.co/api/v2/type/water\", this.water);\n    },\n\n    loadDialog(id) {\n      this.pokemon_ID = id;\n      this.dialog = true;\n      this.pokemon_abilities = [];\n      this.pokemon_types = [];\n      this.getInfoDialog();\n    },\n\n    getInfoDialog() {\n      this.typePokemons.forEach((element) => {\n        element.id = element.pokemon.url\n          .split(\"/\")\n          .filter(function (part) {\n            return !!part;\n          })\n          .pop();\n        if (element.id === this.pokemon_ID) {\n          this.dialogUrl = element.pokemon.url;\n        }\n      });\n      this.getPokemonInfo();\n    },\n    getPokemonInfo() {\n      getPokemons(this.dialogUrl).then((res) => {\n        res.abilities.forEach((element) => {\n          this.pokemon_abilities.push(element.ability.name);\n        });\n        res.types.forEach((element) => {\n          this.pokemon_types.push(element.type.name);\n        });\n        this.pokemon_baseExp = res.base_experience;\n        this.pokemon_height = res.height;\n        this.pokemon_weight = res.weight;\n        this.pokemon_name = res.name;\n      });\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Combobox - type of Pokemon */\n#selectType {\n  display: grid;\n  grid-template-columns: 20% 60% 20%;\n  grid-template-rows: auto;\n  justify-items: center;\n}\n/* Cards - pokemon list */\n#cards {\n  display: grid;\n  grid-template-columns: 25% 25% 25% 25%;\n  grid-template-rows: auto;\n  grid-row-gap: 20px;\n  justify-items: stretch;\n  align-items: stretch;\n}\n\n.card {\n  height: 180px;\n  width: 180px;\n}\n\n.image {\n  width: 155px;\n  height: 155px;\n}\n\n.v-card--reveal {\n  align-items: center;\n  bottom: 0;\n  justify-content: center;\n  opacity: 0.85;\n  position: absolute;\n  width: 100%;\n}\n\n#names {\n  font-size: 15px;\n  font-weight: 800;\n  font-family: \"Press Start 2P\", cursive;\n}\n\n/* Dialog component */\n#dialogInfo {\n  display: grid;\n  justify-items: center;\n  align-items: center;\n}\n\n#titleDialog {\n  font-family: \"Press Start 2P\", cursive;\n  font-size: 14px;\n}\n#abilities {\n  padding: 10px;\n  text-align: center;\n}\n\n.pokemonDetail {\n  font-size: 16px;\n  display: grid;\n  grid-template-columns: 100%;\n  grid-template-rows: auto;\n  justify-items: center;\n  align-items: center;\n}\n\n@media screen and (max-width: 630px) {\n  #cards {\n    grid-template-columns: 100%;\n    grid-row-gap: 15px;\n  }\n\n  #selectType {\n    grid-template-columns: 100%;\n    grid-row-gap: 10px;\n    grid-template-rows: auto;\n    justify-items: center;\n    align-items: center;\n    border: solid 20px transparent;\n  }\n\n  .card {\n    height: 150px;\n    width: 150px;\n  }\n\n  .image {\n    width: 120px;\n    height: 120px;\n  }\n\n  #names {\n    font-weight: 600;\n    font-size: 11px;\n  }\n}\n\n@media screen and (min-width: 630px) and (max-width: 800px) {\n  #cards {\n    grid-template-columns: 50% 50%;\n    grid-row-gap: 12px;\n  }\n\n  .card {\n    height: 150px;\n    width: 150px;\n  }\n\n  .image {\n    width: 120px;\n    height: 120px;\n  }\n\n  #names {\n    font-weight: 600;\n    font-size: 11px;\n  }\n}\n\n@media screen and (min-width: 800px) and (max-width: 1000px) {\n  #cards {\n    grid-template-columns: 33% 33% 33%;\n    grid-row-gap: 12px;\n  }\n}\n</style>\n"]}]}