 dense\n            append-icon=\"mdi-magnify\"\n            color=\"primary\"\n          ></v-text-field>\n          <v-spacer></v-spacer>\n          <v-btn>Filtr</v-btn>\n          </div>\n\n    <div id=\"cards\">\n      <div v-for=\"pokemon in getPokemon\" :key=\"pokemon.name\" >\n        <v-hover v-slot:default=\"{ hover }\">\n          <v-card class=\"mx-auto d-flex flex-wrap elevation-6 card\" text raised @click.stop=\"loadDialog(pokemon.id)\">\n            \n            <v-col cols=\"auto\">\n          <v-img class=\"align-end image\" :src=\"`https://pokeres.bastionbot.org/images/pokemon/${pokemon.id}.png`\">\n            </v-img>\n        </v-col>\n        <v-expand-transition>\n            <div\n              v-if=\"hover\"\n              class=\"d-flex transition-fast-in-fast-out red v-card--reveal white--text\"\n              id=\"names\"\n            >\n            {{pokemon.name}}\n          </div>\n        </v-expand-transition>\n          </v-card>\n        </v-hover>\n      </div>\n    </div>\n      \n      <!-- DIALOG -->\n      <v-dialog\n        v-model=\"dialog\" max-width=\"450\" max-height=\"450\"\n      >\n      <v-card width=\"450\"\n      height=\"450\" class=\"elevation-12\">\n      <v-col cols=\"auto\" id=\"detailInfo\">\n      <v-spacer></v-spacer>\n      <v-avatar size=\"120\" class=\"grey lighten-3\">\n        <v-img class=\"align-end\" :src=\"`https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/shiny/${this.pokemon_ID}.png`\" \n             width=\"150\" height=\"150\"></v-img>\n             </v-avatar>\n          \n             <v-card-title id=\"titleDialog\">{{ this.pokemon_name }} \n             </v-card-title>\n             <v-card-subtitle><v-chip color=\"error\" v-for=\"type in this.pokemon_types\" :key=\"type\"> {{ type }}  </v-chip></v-card-subtitle>\n             <v-row></v-row>\n\n        <v-card-text class=\"pokemonInfo\">\n          <v-row>Base experience: {{ this.pokemon_baseExp }} XP</v-row>\n          <v-row>Height: {{ this.pokemon_height/10 }} m</v-row>\n          <v-row>Weight: {{ this.pokemon_weight/10 }} kg</v-row>\n          \n           <v-row>Abilities</v-row>\n           <v-row><v-chip color=\"warning\" v-for=\"ability in this.pokemon_abilities\" :key=\"ability\"> {{ ability }}  </v-chip></v-row>\n        </v-card-text>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn\n            color=\"green darken-1\"\n            text\n            @click=\"dialog = false\"\n          >\n            Choose me!\n          </v-btn>\n        </v-card-actions>\n      </v-col>\n      </v-card>\n    </v-dialog>\n\n    </v-container>\n    \n\n</template>\n\n<script>\nimport store from '../store/index'\nimport { getPokemons } from '../api/api'\n\n  export default {\n    data() {\n      return {\n        dialog: false,\n        noSearchedPokemon: 0,\n\n        pokemon_ID: null,\n        pokemon_name: '',\n        pokemon_baseExp: null,\n        pokemon_weight: null,\n        pokemon_height: null,\n        pokemon_abilities: [],\n        pokemon_types: [],\n\n        searchedName: null,\n        dialogUrl: null,\n      }\n      \n    },\n    computed: {\n      getPokemon() {\n        return store.getters.pokemons;\n      },\n\n      getNextUrl() {\n        return store.getters.nextUrl\n      },\n      getPrevUrl() {\n        return store.getters.prevUrl\n      },\n    },\n\n  methods: {\n    \n    keywordChanged() {\n      this.pokemon_abilities = []\n      this.pokemon_types = []\n      console.log(`Name is ${this.searchedName}.`)\n      getPokemons('https://pokeapi.co/api/v2/pokemon?offset=0&limit=964')\n        .then((res) => {\n          let i = 0\n          res.results.forEach(pokemon => {\n            if (pokemon.name === this.searchedName) {\n              pokemon.id = pokemon.url.split('/').filter(function(part) { return !!part }).pop();\n              this.dialogUrl = pokemon.url\n              this.pokemon_ID = pokemon.id\n              this.dialog = true\n              this.searchedName = ''\n              this.noSearchedPokemon = i\n            }\n            i++\n          });\n          \n\n          //jat\n          getPokemons(`https://pokeapi.co/api/v2/pokemon?offset=${parseInt(this.noSearchedPokemon - this.noSearchedPokemon%12)}&limit=12`)\n          .then((res) => {\n          \n          console.log('JT refresh')\n          store.commit(\"SET_NEXT_URL\", res.next)\n          store.commit(\"SET_NEXT_URL\", res.previous)\n          console.log('Search next url', res.next)\n          console.log('Search prev url', res.previous)\n          this.pokemons = []\n          res.results.forEach(pokemon => {\n              pokemon.id = pokemon.url.split('/').filter(function(part) { return !!part }).pop();\n              //pokemon.offset = this.offset\n              this.pokemons.push(pokemon);\n            });\n            store.commit(\"SET_POKEMONS\", this.pokemons)\n            console.log(this.pokemons)\n            \n        })\n          //jat\n          \n          getPokemons(this.dialogUrl)\n          .then((res) => {\n          console.log(res.types)\n        res.abilities.forEach(element => {\n          console.log(element.ability.name)\n          this.pokemon_abilities.push(element.ability.name)\n        });\n        res.types.forEach(element => {\n          this.pokemon_types.push(element.type.name)\n          console.log(element.type.name)\n        });\n        this.pokemon_baseExp = res.base_experience\n        this.pokemon_height = res.height\n        this.pokemon_weight = res.weight\n        this.pokemon_name = res.name\n        })\n        })\n    },\n\n    loadDialog(id) {\n      this.pokemon_ID = id\n      this.dialog = true\n      this.pokemon_abilities = []\n      this.pokemon_types = []\n      this.getInfo()\n    },\n\n    getInfo() {\n      store.getters.pokemons.forEach(pokemon => {\n        if (pokemon.id === this.pokemon_ID) {\n          this.dialogUrl = pokemon.url\n        }\n      });\n      getPokemons(this.dialogUrl)\n      .then((res) => {\n        console.log(res.types)\n        res.abilities.forEach(element => {\n          console.log(element.ability.name)\n          this.pokemon_abilities.push(element.ability.name)\n        });\n        res.types.forEach(element => {\n          this.pokemon_types.push(element.type.name)\n          console.log(element.type.name)\n        });\n        this.pokemon_baseExp = res.base_experience\n        this.pokemon_height = res.height\n        this.pokemon_weight = res.weight\n        this.pokemon_name = res.name\n        \n      })\n    }\n  }\n  }\n\n  \n</script>\n\n<style  scoped>\n@import url('https://fonts.googleapis.com/css?family=Acme|Fredoka+One|Fugaz+One|Gochi+Hand|Lexend+Giga|Monoton|Press+Start+2P&display=swap');\n\n#search {\n    display: grid;\n    grid-template-columns: 60% 25% 10%;\n    grid-template-rows: auto;\n    justify-items: flex-end;\n    align-items:flex-start;\n\n}\n\n#cards {\n    display: grid;\n    grid-template-columns: 25% 25% 25% 25%;\n    grid-template-rows: auto;\n    grid-row-gap: 20px;\n    justify-items: stretch;\n    align-items: stretch;\n}\n\n.card {\n  height: 180px;\n  width: 180px;\n}\n\n#detailInfo {\n  display: grid;\n  grid-template-columns: 100%;\n  grid-template-rows: auto;\n  justify-items: center;\n  align-items: center;\n}\n\n.pokemonInfo {\n  font-size: 16px;\n  display: grid;\n  grid-template-columns: 100%;\n  grid-template-rows: auto;\n  justify-items: center;\n  align-items: center;\n}\n\n.image {\n  width: 155px;\n  height: 155px;\n}\n\n#names {\n  font-size: 14px;\n  font-weight: 800;\n  font-family: 'Press Start 2P', cursive;\n}\n\n.v-card--reveal {\n  align-items: center;\n  bottom: 0;\n  justify-content: center;\n  opacity: 0.85;\n  position: absolute;\n  width: 100%;\n}\n\n#titleDialog {\n  font-family: 'Press Start 2P', cursive;\n  font-size: 14px;\n}\n\n@media screen and (max-width: 700px) {\n  #cards {\n    grid-template-columns: 50% 50%;\n    grid-row-gap: 12px;\n  }\n\n  .card {\n    height: 150px;\n    width: 150px;\n  }\n\n  .image {\n    width: 120px;\n    height: 120px;\n  }\n\n  #names {\n    font-weight: 600;\n    font-size: 11px;\n  }\n}\n\n\n@media screen and (min-width: 700px) and (max-width: 900px) {\n  #cards {\n    grid-template-columns: 33% 33% 33%;\n    grid-row-gap: 12px;\n  }\n}\n\n\n\n</style>"]}]}